<odoo>
    <data>
        <record id="purchase_sequence" model="ir.sequence">
            <field name="name">Purchase Request</field>
            <field name="code">purchase.request.order</field>
            <field name="prefix">PR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="purchase_rfq_sequence_1" model="ir.sequence">
            <field name="name">Purchase RFQ</field>
            <field name="code">purchase.rfq</field>
            <field name="prefix">PRFQ</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>