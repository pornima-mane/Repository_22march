<odoo>
	<data>
		<record id="view_wizard_vendor_form" model="ir.ui.view">
			<field name="name">view.wizard.wizard_vendor.form</field>
			<field name="model">wizard_vendor</field>
			<field name="arch" type="xml">
				<form>
					<!-- <group col="1"> <lable string="message" /> </group> -->

					<group col="4">
						<field name="vendors" widget="many2many_tags"/>
						<field name="company_id" invisible="1"/>
					</group>
					<footer>
						<button name="confirm_sale" type="object" string="Confirm"
							class="oe_highlights" />
						<button string="Cancel" class="oe_highlights" special="cancel" />
					</footer>
				</form>
			</field>
		</record>
	</data>
</odoo>
